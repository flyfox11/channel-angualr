<!DOCTYPE html>
<html>
<%- include ../public/header %>
<body>
<style>
    /*改变tab所占的宽度*/
    .investment_title a, .investment_title a:link, .investment_title a:visited {
        padding: 0px 25px;
    }
    .table_td{
        width:40%;
    }
</style>
<div class="wrapper">
    <%- include ../public/sidebar %>
    <div class="main-panel">
        <%- include ../public/nav %>
        <div class="investment_f">
            <% include ./menu %>
            <div id="myTabContent" class="tab-content" style=" padding: 20px;">
                <div class="tab-pane fade in active" id="customerInfo">
                    <!--<div class="investment_title" style="margin-top:20px;margin-bottom:20px">-->
                        <!--<div class="on">-->
                            <!--<a href="/system/channelInfo">渠道信息</a>-->
                        <!--</div>-->
                        <!--<div>-->
                            <!--<a href="/system/userInfo">登录用户信息</a>-->
                        <!--</div>-->
                    <!--</div>-->
                    <div style="text-align: center;font-size: 16px;font-weight: bolder;margin: 10px 0;">渠道信息</div>
                    <table style="text-align: center" class="table table-striped table-hover table-condensed table-bordered">
                        <tbody>
                        <tr>
                            <td class="table_td">渠道编号</td>
                            <td><%= data.channel_no %></td>
                        </tr>
                        <tr>
                            <td class="table_td">渠道名称</td>
                            <td><%= data.name %></td>
                        </tr>
                        <tr>
                            <td class="table_td">组织机构号</td>
                            <td><%= data.org_cd %></td>
                        </tr>
                        </tbody>
                    </table>
                    <div style="text-align: center;font-size: 16px;font-weight: bolder;margin: 30px 0 10px 0;">登录用户信息</div>
                    <table style="text-align: center"
                           class="table table-striped table-hover table-condensed table-bordered">
                        <tbody>
                        <tr>
                            <td class="table_td">姓名</td>
                            <td><%= locals.session.user.crealname %></td>
                        </tr>
                        <tr>
                            <td class="table_td">电话</td>
                            <td><%= locals.session.user.cphone %></td>
                        </tr>
                        <tr>
                            <td class="table_td">邮箱</td>
                            <td><%= locals.session.user.cemail %></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include ../public/errorPagePop %>
</body>
</html>
