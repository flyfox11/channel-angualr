<script>
    $("body").on("click", "#roleData tr", function () {
        if (!$(this).hasClass('tabalTrActive') && $(this).find('td').length > 1) {
            $(this).addClass('tabalTrActive').siblings().removeClass('tabalTrActive');
            $("#upload").addClass('btn-success').removeClass('btn-default');
        } else {
            $(this).removeClass('tabalTrActive');
            $("#upload").removeClass('btn-success');
        }
    });
    $("#upload").click(function () {
        //btn-success btn-default
        if ($(this).hasClass('btn-success')) {
            $('#block_box_create1').modal('show');
        } else {
            jDialog.alert('请先选择项目!');
        }
    });
